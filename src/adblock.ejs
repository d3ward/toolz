			<section class="page-active" id="adb_test">
				<div class="grid">
					<div>
						<hr>
					</div>
					<div id="cf_wrap">
						<div class="test_card">
							<h5>
								<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" stroke-width="2"
									stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
